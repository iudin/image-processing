{
  "env": {
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["prettier", "plugin:prettier/recommended"],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "func-style": ["warn", "expression"]
  }
}
